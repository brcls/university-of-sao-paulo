#include <stdio.h>

int main(void){
   
      // PONTOS PARA CRISTINA
      int Cristina = 0, Bruno = 0, Andreia = 0;
      int a,b,c,d,e,f;
    
    scanf("%d", &a);
    scanf("%d", &b);
    scanf("%d", &c);
    scanf("%d", &d);
    scanf("%d", &e);
    scanf("%d", &f);

    Andreia=a+b;
    Bruno=c+d;
    Cristina=e+f;
    
    if (a < 1 || a > 6){
    printf("Alguém trapaceou nesse jogo!");
    return 0;
    } 
    if (b < 1 || b > 6){
    printf("Alguém trapaceou nesse jogo!");
    return 0;
    } 
    if (c < 1 || c > 6){
    printf("Alguém trapaceou nesse jogo!");
    return 0;
    } 
    if (d < 1 || d > 6){
    printf("Alguém trapaceou nesse jogo!");
    return 0;
    } 
    if (e < 1 || e > 6){
    printf("Alguém trapaceou nesse jogo!");
    return 0;
    } 
    if (f < 1 || f > 6){
    printf("Alguém trapaceou nesse jogo!");
    return 0;
    } 
    
   if (Bruno>Andreia){

         if (Bruno > Cristina) {

            if(Andreia>Cristina) printf("Bruno\nAndreia\nCristina");
            else printf("Bruno\nCristina\nAndreia");
         }
         else { // CRISTINA > BRUNO
            printf("Cristina\nBruno\nAndreia");
         }
   }
   else 
   
   if(Andreia>Cristina){
        
      if(Andreia > Bruno) {
         if(Cristina>Bruno) printf("Andreia\nCristina\nBruno");
         else printf("Andreia\nBruno\nCristina");   
      }
      else {
         printf("Bruno\nAndreia\nCristina");
      }
   }
   else {
      if(Cristina > Bruno) {
         if (Cristina > Andreia) {
            if (Bruno > Andreia) printf("Cristina\nBruno\nAndreia");
            else printf("Cristina\nAndreia\nBruno");
         }
         else {
         printf("Andreia\nCristina\nBruno");
         }
      }
   }
    
   return 0;
}

#include <stdio.h>

int main(void){
   
   float p1;
   float p2;
   float p3;
   float f;
   double m;
   
   scanf("%f", &p1);
   scanf("%f", &p2);
   scanf("%f", &p3);
   scanf("%f", &f);

   m=(p1+p2+p3)/3;
   
   if(m>=5 && f>=70){
	   printf("Aprovado");
   }
   if(3<=m && m<5 && f>=70){
	   printf("Recuperação");
   }
   if(m<3 || f<70){
	   printf("Reprovado");
   }

return 0;
}
